    "@prisma/client": "^6.12.0",
    "@tanstack/react-query": "^5.83.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.14",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "nodemailer": "^7.0.5",
    "path-to-regexp": "^0.1.12",
    "prisma": "^6.12.0",
    "react-beautiful-dnd": "^13.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.0",
    "@types/nodemailer": "^7.0.1",
    "concurrently": "^9.2.0",
    "maildev": "^2.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3"
  }
}
