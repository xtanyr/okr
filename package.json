{
  "name": "okr",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "build:frontend": "cd frontend && npm run build",
    "start:frontend": "cd frontend && npx serve -s dist -l 3000",
    "start:frontend:port": "cd frontend && npx serve -s dist -l",
    "build:all": "npm run build && npm run build:frontend",
    "start:unified": "node server.js",
    "start:simple": "node simple-start.js",
    "start:ts": "tsx start-simple.ts",
    "start:minimal": "node minimal-server.js",
    "start:ultra": "node ultra-simple.js",
    "start:native": "node native-server.js",
    "build:unified": "npm run build:all && npm run start:unified",
    "pm2:start": "pm2 start start-simple.ts --name \"okr-app\" --interpreter=\"node\" --interpreter-args=\"--loader tsx\"",
    "pm2:restart": "pm2 restart okr-app",
    "pm2:stop": "pm2 stop okr-app",
    "pm2:delete": "pm2 delete okr-app",
    "pm2:logs": "pm2 logs okr-app"
  },
  "keywords": [],
